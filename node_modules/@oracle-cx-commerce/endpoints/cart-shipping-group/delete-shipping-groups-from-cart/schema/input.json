{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-shipping-group/deleteShippingGroups/input.json",
  "title": "delete Shipping Groups",
  "description": "delete multiple shipping groups of the current order",
  "type": "object",
  "properties": {
    "shippingGroupIds": {
      "description": "list of shipping group ids to be deleted",
      "type": "array",
      "items": {
        "description": "shipping group ID",
        "type": "string"
      }
    }
  },
  "required": [
    "shippingGroupIds"
  ],
  "example": {
    "shippingGroupIds": ["sg40421", "sg40422"]
  }
}