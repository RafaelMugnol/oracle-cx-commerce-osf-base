  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-payment/getBillingCountries/output.json",
  "title": "Get Billing Countries",
  "description": "Lists all the configured billing countries.",
  "type": "object",
  "properties": {
    "countryRegionRepository": {
      "type": "object",
      "description": "The Country and Region Repository",
      "properties": {
        "billingCountries": {
          "type": "object",
          "description": "Billing countries list",
          "patternProperties": {
            "^[A-Z][A-Z]$": {
              "description": "Country information and associated regions. Property ID is represented by ISO 3166-1 two letter country code",
              "type": "object",
              "properties": {
                "repositoryId": {
                  "type": "string",
                  "description": "The ID of the country item."
                },
                "displayName": {
                  "type": "string",
                  "description": "Localized country name."
                },
                "countryCode": {
                  "type": "string",
                  "description": "ISO 3166-1 two letter country code."
                },
                "regions": {
                  "type": "array",
                  "description": "The list of regions for particular country.",
                  "items": {
                    "type": "object",
                    "description": "",
                    "properties": {
                      "repositoryId": {
                        "type": "string",
                        "description": "The ID of the region item."
                      },
                      "regionCode": {
                        "type": "string",
                        "description": "The region code (4 character ISO code)."
                      },
                      "abbreviation": {
                        "type": "string",
                        "description": "The region code in abbreviated form. Usually a 2 character ISO code"
                      },
                      "displayName": {
                        "type": "string",
                        "description": "Localized region name."
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "$ref": "@oracle-cx-commerce/endpoints/references/error.json#/error"
    }
  },
