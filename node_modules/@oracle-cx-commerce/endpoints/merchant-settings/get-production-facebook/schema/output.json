{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/init-merchant-settings/getProductionFacebook/output.json",
  "type": "object",
  "title": "Get production Facebook response",
  "description": "Get production Facebook response.",
  "properties": {
    "merchantRepository": {
      "type": "object",
      "description": "The merchant Repository",
      "properties": {
        "settings": {
          "type": "object",
          "description": "Merchant settings",
          "properties": {
            "productionFacebook": {
              "type": "object",
              "description": "Production Facebook",
              "properties": {
                "displayName": {
                  "type": "string",
                  "description": "Display name of the Production Facebook setting"
                },
                "name": {
                  "type": "string",
                  "description": "Name of the Production Facebook setting"
                },
                "applicationId": {
                  "type": "string",
                  "description": "Application Id of Production Facebook"
                }
              }
            }
          }
        }
      }
    }
  },
  "example": {
    "merchantRepository": {
      "settings": {
        "productionFacebook": {
          "displayName": "Facebook",
          "name": "Facebook",
          "applicationId": "123456"
        }
      }
    }
  }
}
