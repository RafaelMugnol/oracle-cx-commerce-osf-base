{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-organization-address/getOrganizationDefaultAddresses/output.json",
  "title": "Get Organization Default Addresses",
  "description": "This operation is used to get the default shipping and billing address of a Organization/Account Oracle Commerce Cloud.",
  "type": "object",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "The Profile Repository",
      "properties": {
        "organizations": {
          "description": "Get organization",
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "active": {
                "description": "Active status of an Organization. Should be true or false. By default the value is set to true",
                "type": "boolean"
              },
              "name": {
                "description": "The name for Organization",
                "type": "string"
              },
              "repositoryId": {
                "description": "Repository Id of the Organization",
                "type": "string"
              },
              "id": {
                "description": "Id of the Organization",
                "type": "string"
              },
              "shippingAddress": {
                "description": "Default shipping address of the organization",
                "type": "string"
              },
              "billingAddress": {
                "description": "Default billing address of the organization",
                "type": "string"
              }
            }
          }
        },
        "contactInfos": {
          "type": "object",
          "description": "The Addresses",
          "properties": {
            "contactInfo": {
              "type": "object",
              "description": "The Address",
              "properties": {
                "country": {
                  "description": "The country of the address",
                  "type": "string"
                },
                "phoneNumber": {
                  "description": "Phone Number of the address",
                  "type": "string"
                },
                "address2": {
                  "description": "The address2 of the secondary address",
                  "type": "string"
                },
                "city": {
                  "description": "The city of the secondary address",
                  "type": "string"
                },
                "address1": {
                  "description": "The address1 of the secondary address",
                  "type": "string"
                },
                "companyName": {
                  "description": "Company name of the secondary address",
                  "type": "string"
                },
                "postalCode": {
                  "description": "Postal Code of the address",
                  "type": "string"
                },
                "repositoryId": {
                  "description": "Repository id of the address. Deprecated this property. Use id instead",
                  "type": "string"
                },
                "id": {
                  "description": "Repository id of the address. This field should be used inorder to support sort on the repository id",
                  "type": "string"
                },
                "state": {
                  "description": "The State of the address",
                  "type": "string"
                },
                "addressType": {
                  "description": "Address type represents nick name of account if present else repository ID of address item.",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "message": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "profileRepository": {
      "organizations": {
        "or-100003": {
          "repositoryId": "or-100003",
          "name": "US Motor Works - US Division",
          "active": true,
          "description": "US Motor Works - US Division",
          "links": [
            {
              "rel": "self",
              "href": "http://localhost:8080/ccstore/v1/organizationAddresses"
            }
          ],
          "id": "or-100003",
          "billingAddress": "ci-110029",
          "shippingAddress": "ci-110029"
        }
      },
      "contactInfos": {
        "ci-110029": {
          "country": "US",
          "phoneNumber": "408-752-3500",
          "address3": null,
          "city": "San Jose",
          "address2": "Santa Clara County",
          "address1": "7 W Jackson Blvd",
          "postalCode": "95111",
          "companyName": "US Motor Works, Inc.",
          "repositoryId": "ci-110029",
          "county": null,
          "state": "CA",
          "id": "ci-110029"
        }
      }
    },
    "error": {
      "errorCode": "1000500",
      "message": "Invalid input",
      "status": "500"
    }
  }
}
