{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-organization/listOrganizations/output.json",
  "title": "List Organizations",
  "description": "Following model is returned when the list organizations operation responds.",
  "type": "object",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "Profile Repository",
      "properties": {
        "organizations": {
          "description": "List of organizations for a given query.",
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "description": "list of organizations",
              "type": "object",
              "properties": {
                "name": {
                  "description": "Organization name.",
                  "type": "string"
                },
                "repositoryId": {
                  "description": "Repository id of organization.",
                  "type": "string"
                },
                "active": {
                  "description": "Indicates whether organization active or not.",
                  "type": "boolean"
                }
              }
            }
          }
        },
        "roles": {
          "description": "List of relative roles part of organization",
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "function": {
                "description": "Roles corresponding to relativeTo organization",
                "type": "string"
              },
              "relativeTo": {
                "description": "Role relative to organization",
                "type": "string"
              },
              "repositoryId": {
                "description": "Id of role",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "errorMessage": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "profileRepository": {
      "organizations": {
        "or-100001": {
          "active": true,
          "name": "National Discount Auto Parts",
          "repositoryId": "or-100001"
        },
        "or-100002": {
          "active": true,
          "name": "US Motor Works, Inc.",
          "repositoryId": "or-100002"
        },
        "or-100003": {
          "active": true,
          "name": "US Motor Works - US Division",
          "repositoryId": "or-100003"
        }
      },
      "roles": {
        
      }
    },
    "error": {
      "errorCode": "1000500",
      "errorMessage": "Invalid input",
      "status": "500"
    }
  }
}