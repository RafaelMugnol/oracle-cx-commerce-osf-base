{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-profile/getProfileAddress/output.json",
  "type": "object",
  "title": "GET Profile Address response",
  "description": "GET Profile Address response.",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "The Profile Repository",
      "properties": {
        "contactInfos": {
          "type": "object",
          "description": "The Profiles",
          "properties": {
            "contactInfo": {
              "type": "object",
              "description": "The Address",
              "properties": {
                "addressType": {
                  "description": "The alias of the shipping address.",
                  "type": "string"
                },
                "firstName": {
                  "description": "The first name of the shipping address.",
                  "type": "string"
                },
                "lastName": {
                  "description": "The last name of the shipping address.",
                  "type": "string"
                },
                "address1": {
                  "description": "The address line 1 of the shipping address.",
                  "type": "string"
                },
                "address2": {
                  "description": "The address line 2 of the shipping address.",
                  "type": "string"
                },
                "address3": {
                  "description": "The address line 3 of the shipping address.",
                  "type": "string"
                },
                "city": {
                  "description": "The city of the shipping address.",
                  "type": "string"
                },
                "county": {
                  "description": "The county code of the shipping address.",
                  "type": "string"
                },
                "state": {
                  "description": "The state code of the shipping address.",
                  "type": "string"
                },
                "postalCode": {
                  "description": "The postal Code of the shipping address.",
                  "type": "string"
                },
                "country": {
                  "description": "The country of the shipping address.",
                  "type": "string"
                },
                "phoneNumber": {
                  "description": "The phone Number of the shipping address.",
                  "type": "string"
                },
                "companyName": {
                  "description": "The company name of the shipping address.",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "message": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "profileRepository": {
      "contactInfos": {
        "ci-12345": {
          "addressType": "Address",
          "firstName": "Jack",
          "lastName": "Black",
          "address1": "405 Example Street",
          "address2": "",
          "address3": "",
          "city": "Wake Forest",
          "county": "",
          "state": "NC",
          "postalCode": "27587",
          "country": "US",
          "phoneNumber": "555123456",
          "companyName": ""
        }
      }
    },
    "error": {
      "errorCode": "1000500",
      "message": "Invalid input",
      "status": "500"
    }
  }
}