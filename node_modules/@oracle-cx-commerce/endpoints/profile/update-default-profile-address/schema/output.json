{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-profile/updateDefaultProfileAddress/output.json",
  "type": "object",
  "title": "Update Profile Address as default",
  "description": "Update Profile Address as default",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "The Profile Repository",
      "properties": {
        "profiles": {
          "type": "object",
          "description": "The Profiles",
          "properties": {
            "profile": {
              "type": "object",
              "description": "The Profile",
              "properties": {
                "shippingAddress": {
                  "description": "The default shipping address to updated to null if default shipping address deleted.",
                  "type": "string"
                },
                "contactShippingingAddress": {
                  "description": "The default shipping address to updated to null if default shipping address deleted.",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "$ref": "@oracle-cx-commerce/endpoints/references/error.json#/error"
    }
  },
  "example": {
    "profileRepository": {
     "profiles": {
        "123456": {
          "shippingAddress": "247342",
          "contactShippingAddress": "247342"
        }
      }
    },
    "error": {
      "errorCode": "1000500",
      "message": "Invalid input",
      "status": "500"
    }
  }
}