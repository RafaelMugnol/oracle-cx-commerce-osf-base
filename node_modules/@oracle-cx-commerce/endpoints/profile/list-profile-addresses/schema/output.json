{
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-profile/listProfileAddresses/output.json",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "title": "List Profile Addresses",
  "description": "List Profile Addresses specific to the customer profile.",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "The Profile Repository",
      "properties": {
        "contactInfos": {
          "type": "object",
          "description": "List of shipping addresses for profile",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "type": "object",
              "description": "List of secondary addresses for particular profile. Here object key is profieId",
              "patternProperties": {
                "^[a-zA-Z1-9]+$": {
                  "type": "object",
                  "description": "The Address object. Here key of object is addressType",
                  "properites": {
                    "firstName": {
                      "description": "The first name of the address.",
                      "type": "string"
                    },
                    "lastName": {
                      "description": "The last name of the address.",
                      "type": "string"
                    },
                    "country": {
                      "description": "the country code of the address.",
                      "type": "string"
                    },
                    "phoneNumber": {
                      "description": "The phone Number of the address.",
                      "type": "string"
                    },
                    "address2": {
                      "description": "The address line 2 of the address.",
                      "type": "string"
                    },
                    "address3": {
                      "description": "The address line 3 of the address.",
                      "type": "string"
                    },
                    "city": {
                      "description": "the city of the address.",
                      "type": "string"
                    },
                    "address1": {
                      "description": "The address line 1 of the address.",
                      "type": "string"
                    },
                    "postalCode": {
                      "description": "The postal Code of the address.",
                      "type": "string"
                    },
                    "companyName": {
                      "description": "The company name of the address.",
                      "type": "string"
                    },
                    "repositoryId": {
                      "description": "The repository ID of the shipping address.",
                      "type": "string"
                    },
                    "id": {
                      "description": "The ID of the shipping address. Same as repositoryId",
                      "type": "string"
                    },
                    "county": {
                      "description": "The county of the address.",
                      "type": "string"
                    },
                    "state": {
                      "description": "The state code of the address.",
                      "type": "string"
                    },
                    "prefix": {
                      "description": "The prefix of the address.",
                      "type": "string"
                    },
                    "suffix": {
                      "description": "The suffix of the address.",
                      "type": "string"
                    },
                    "faxNumber": {
                      "description": "The fax Number of the address.",
                      "type": "string"
                    },
                    "middleName": {
                      "description": "The Middle Name of the address.",
                      "type": "string"
                    },
                    "jobTitle": {
                      "description": "The job Title of the address.",
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "$ref": "@oracle-cx-commerce/endpoints/references/error.json#/error"
    }
  },
  "example": {
    "error": {
      "errorCode": "00001000",
      "message": "The user is not authenticated.",
