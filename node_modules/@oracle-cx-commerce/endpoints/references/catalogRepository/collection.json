{
    "catalogRepository": {
      "type": "object",
      "description": "Catalog Repository",
      "properties": {
        "categories": {
          "description": "Get collection identified by collection id.",
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "description": "",
              "type": "object",
              "properties": {
                "longDescription": {
                  "description": "Long description of the collection.",
                  "type": "string"
                },
                "route": {
                  "description": "Route of the collection.",
                  "type": "string"
                },
                "dimensionId": {
                  "description": "Dimension ID.",
                  "type": "string"
                },
                "displayName": {
                  "description": "Display Name of the collection.",
                  "type": "string"
                },
                "multiCatalogCategoryPaths": {
                  "description": "An array of collection paths prepended with catalog name",
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "multiCatalogCategoryIdPaths": {
                  "description": "An array of collection ids in the path prepended with catalog id",
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "repositoryId": {
                  "description": "Repository ID.",
                  "type": "string"
                },
                "childCategories": {
                  "description": "Array of collection objects.",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "longDescription": {
                        "description": "Long description of the child collection.",
                        "type": "string"
                      },
                      "route": {
                        "description": "Route of the child collection.",
                        "type": "string"
                      },
                      "displayName": {
                        "description": "Display name of the child collection.",
                        "type": "string"
                      },
                      "id": {
                        "description": "ID of the child collection.",
                        "type": "string"
                      },
                      "creationDate": {
                        "description": "product creation date",
                        "type": "string"
                      }
                    }
                  }
                },
                "id": {
                  "description": "collection ID.",
                  "type": "string"
                },
                "creationDate": {
                  "description": "product creation date",
                  "type": "string"
                },
                "fixedParentCategories": {
                  "description": "Array of collection objects.",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "repositoryId": {
                        "description": "Repository id of parent collection.",
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
