{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/store-cart/listAccessRights/input.json",
  "title": "Lists Access Rights Details",
  "description": "Lists AccessRights details. This is used to get existing accessRights",
  "type": "object",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "Profile Repository",
      "properties": {
        "accessRights": {
          "type": "object",
          "description": "List of accessRights details",
          "patternProperties": {
            "^[a-zA-Z0-9_.-]+$": {
              "description": "accessRights Id. Here object key is accessRightId",
              "type": "object",
              "properties": {
                "description": {
                  "description": "accessRights Description",
                  "type": "string"
                },
                "name": {
                  "description": "accessRights name",
                  "type": "string"
                },
                "repositoryId": {
                  "description": "Repository Id details of accessRights",
                  "type": "string"
                },
                "type": {
                  "description": "accessRights type",
                  "type": "string"
                },
                "id": {
                  "description": "id of accessRights",
                  "type": "string"
                },
                "displayName": {
                  "description": "display name for accessRights",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "json": {
      "type": "array",
      "description": "array of access rights",
      "items": {
        "type": "object",
        "description": "accessRight object",
        "properties": {
          "description": {
            "description": "accessRights Description",
            "type": "string"
          },
          "name": {
            "description": "accessRights name",
            "type": "string"
          },
          "repositoryId": {
            "description": "Repository Id details of accessRights",
            "type": "string"
          },
          "type": {
            "description": "accessRights type",
            "type": "string"
          },
          "id": {
            "description": "id of accessRights",
            "type": "string"
          },
          "displayName": {
            "description": "display name for accessRights",
            "type": "string"
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "errorMessage": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "profileRepository": {
      "accessRights": {
        "createAccountAddress": {
          "name": "Create Account Address",
          "type": "generic",
          "displayName": "",
          "repositoryId": "createAccountAddress",
          "id": "createAccountAddress",
          "description": ""
        }
      }
    }
  }
}