{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-cart/switchCart/output.json",
    "title": "Switch Cart",
    "description": "Provides the ability to switch the current order context to a given incomplete order from the list of saved orders for a logged in shopper. It also supports switching the current order context with a new empty cart order for a registered shopper.",
    "type": "object",
    "properties": {
      "clientRepository": {
        "type": "object",
        "description": "Client Repository",
        "properties": {
          "context": {
            "description": "context object",
            "type": "object",
            "properties": {
                "session": {
                    "description": "sessiojn object",
                    "type": "object",
                    "properties": {
                        "order": {
                            "description": "current orderId",
                            "type": "string"
                        }
                    }
                }
            }
          }
        }
      },
      "error": {
        "type": "object",
        "description": "The error object",
        "properties": {
          "errorCode": {
            "description": "The error code.",
            "type": "string"
          },
          "errorMessage": {
            "description": "The error message.",
            "type": "string"
          },
          "status": {
            "description": "The error status.",
            "type": "string"
          }
        }
      }
    },
    "example": {
      "clientRepositoy": {
          "context": {
              "session": {
                  "order": "o30545"
              }
          }
      },
      "error": {
        "errorCode": "1000500",
        "errorMessage": "Sample error message",
        "status": "500"
      }
    }
  }