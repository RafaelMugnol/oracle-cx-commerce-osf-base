{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-cart/removeCouponsFromCart/input.json",
  "title": "Remove Coupons",
  "description": "Remove Coupons. Removes coupon(s) applied to the order",
  "type": "object",
  "properties": {
    "coupons": {
      "type": "array",
      "description": "array of coupon codes to be removed",
      "items": {
        "type": "string",
        "description": "coupon code"
      }
    }
  },
  "required": [],
  "example": {
    "coupons": ["SAVE2ON20", "FREESHIPPING"]
  }
}