{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-cart/getUpsellMessages/output.json",
  "title": "Get Promotional Upsell Messages",
  "description": "Following model is returned when operation succeeds.",
  "type": "object",
  "properties": {
    "error": {
      "$ref": "@oracle-cx-commerce/endpoints/references/error.json#/error"
    },
    "orderRepository": {
      "type": "object",
      "description": "Order Repository",
      "properties": {
        "pricingMessages": {
          "description": "pricing Message",
          "type": "object",
          "properties": {
            "unqualifiedUpsellMessages": {
              "description": "non Qualified Messages",
              "type": "object",
              "properties": {
                "promotionUpsellMessages": {
                  "description": "Promotion Upsell Messages Array",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "text": {
                        "type": "string",
                        "description": "text messages for promotional upsell messages"
                      },
                      "tags": {
                        "type": "array",
                        "description": "tags defined for promotional Messages",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "example": {
    "orderRepository": {
      "pricingMessages": {
        "unqualifiedUpsellMessages": {
          "promotionUpsellMessages": [
            {
              "tags": [
                "header"
              ],
              "text": "<p>didnt qualify for order add more titems</p>"
            },
            {
              "tags": [],
              "text": "<p>Promotion applied</p>"
            }
          ]
        }
      }
    }
  }
}