{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Log Client Errors",
  "description": "Log Client Errors",
  "type": "object",
  "properties": {
    "body": {
      "description": "The body of the response, an array of cclient side errors",
      "type": "array",
      "items": {
        "clientSideErrors": {
          "type": "object",
          "description": "The client side error",
          "properties": {
            "message":          {"type": "string", "description": "The Error Message"},
            "url":              {"type": "string", "description": "The Error Url"},
            "line":             {"type": "string", "description": "The Error Line Number"},
            "column":           {"type": "string", "description": "The Error Col Number"},
            "clientErrorCode":  {"type": "string", "description": "The Error Name"},
            "osVersion":        {"type": "string", "description": "The Window Navigator Platform"},
            "locale":           {"type": "string", "description": "The Window Navigator Language"},
            "browserVersion":   {"type": "string", "description": "The Browser Version"},
            "browserURL":       {"type": "string", "description": "The Browser URL"},
            "siteId":           {"type": "string", "description": "The Site Id"},
            "time":             {"type": "string", "description": "The Date"},
            "visitorId":        {"type": "string", "description": "The Visitor Id"},
            "visitId":          {"type": "string", "description": "The Visit Id"},
            "errorStack":       {"type": "string", "description": "The Error Stack"}
          }
        }
      }
    }
  },
  "example": {
    "message": "The error message",
    "url": "http://someurl.oracle.com",
    "line": "999",
    "column": "10",
    "clientErrorCode":"ABC123",
    "osVersion": "iOS",
    "locale": "en_GB",
    "browserVersion": "Chrome 1.0.0",
    "browserURL": "http://someurl.oracle.com",
    "siteId": "site_US",
    "time": "240920181010101",
    "visitorId": "123abc",
    "visitId": null,
    "errorStack": null
  }
}
