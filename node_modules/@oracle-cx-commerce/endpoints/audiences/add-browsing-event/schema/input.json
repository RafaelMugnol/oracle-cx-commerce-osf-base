{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-add-browsing-event/addBrowsingEvent/input.json",
  "title": "Add Browsing Event",
  "description": "Track product and collection views for use with behavioral Audiences.",
  "type": "object",
  "properties": {
    "pageView": {
      "type": "object",
      "description": "Object describing the page view.",
      "properties": {
        "catalogId": {
          "description": "ID of the current catalog.",
          "type": "string"
        },
        "id": {
          "description": "ID of the current collection of product.",
          "type": "string"
        },
        "type": {
          "description": "COLLECTION or PRODUCT",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "pageView": {
      "catalogId": "cloudLakeCatalog",
      "id": "Product_35Fxy",
      "type": "PRODUCT"
    }
  }
}
