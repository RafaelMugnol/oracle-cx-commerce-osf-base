{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-scheduled-order/listExecutionStatusForScheduledOrder/output.json",
	"title": "List Execution Status For Scheduled Order",
	"description": "Following model is returned when operation succeeds.",
	"type": "object",
	"properties": {
		"orderRepository": {
			"type": "object",
			"description": "Order Repository",
			"properties": {
				"executionStatus": {
					"description": "List of execution status of scheduled order",
					"type": "object",
					"patternProperties": {
						"^[a-zA-Z1-9]+$": {
							"description": "",
							"type": "object",
							"properties": {
								"executionId": {
									"description": "The execution status representation.",
									"type": "object",
									"properties": {
										"scheduledOrderId": {
											"description": "The ID of the scheduled order.",
											"type": "string"
										},
										"errorMessages": {
											"description": "The error messages list.",
											"type": "string"
										},
										"orderId": {
											"description": "The ID of the template order.",
											"type": "string"
										},
										"repositoryId": {
											"description": "The ID of the execution status.",
											"type": "string"
										},
										"executionDate": {
											"description": "The date for previous scheduled run.",
											"type": "string"
										},
										"id": {
											"description": "The ID of the execution status.",
											"type": "string"
										},
										"orderSubmitted": {
											"description": "The status of scheduled order.",
											"type": "boolean"
										},
										"orderTotal": {
											"description": "Total scheduled order value.",
											"type": "number"
										}
									}
								}
							}
						}
					}
				}
			}
		},
		"error": {
			"type": "object",
			"description": "The error object",
			"properties": {
				"errorCode": {
					"description": "The error code.",
					"type": "string"
				},
				"errorMessage": {
					"description": "The error message.",
					"type": "string"
				},
				"status": {
					"description": "The error status.",
					"type": "string"
				}
			}
		}
	},
	"example": {
		"OrderRepository": {
			"executionStatus": {
				"100001": {
					"scheduledOrderId": "sco30005",
					"errorMessages": [],
					"orderId": "o70411",
					"repositoryId": "100001",
					"executionDate": "2020-04-17T19:20:00.000Z",
					"currency": "USD",
					"id": "100001",
					"orderSubmitted": true,
					"orderTotal": 63.73
				},
				"200001": {
					"scheduledOrderId": "sco30005",
					"errorMessages": ["40001"],
					"orderId": null,
					"repositoryId": "200001",
					"executionDate": "2020-04-20T15:15:00.000Z",
					"currency": "USD",
					"id": "200001",
					"orderSubmitted": false,
					"orderTotal": 63.73
				}
			}
		},
		"error": {
			"errorCode": "1000500",
			"errorMessage": "Sample error message",
			"status": "500"
		}
	}
}
