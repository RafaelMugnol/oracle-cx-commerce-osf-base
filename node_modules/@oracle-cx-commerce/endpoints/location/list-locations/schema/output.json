                    "type": "object",
                    "properties": {
                      "sites": {
                        "description": "List of sites.",
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "description": "The site id.",
                              "type": "string"
                            }
                          }
                        }
                      },
                      "id": {
                        "description": "The site group id.",
                        "type": "string"
                      }
                    }
                  }
                },
                "repositoryId": {
                  "description": "The id of the location repository item.",
                  "type": "string"
                },
                "name": {
                  "description": "The name of the location.",
                  "type": "string"
                },
                "faxNumber": {
                  "description": "The fax number of the location.",
                  "type": "string"
                },
                "email": {
                  "description": "The email id parameter.",
                  "type": "string"
                },
                "longitude": {
                  "description": "The longitude of the location.",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "clientRepository": {
      "type": "object",
      "description": "The Client Repository",
      "properties": {
        "transient": {
          "type": "object",
          "description": "The transient Object",
          "properties": {
            "locationIdsForQuery": {
              "type": "object",
              "description": "location id's for the given query array",
              "properties": {
                "queryForLocations": {
                  "type": "object",
                  "description": "each query",
                  "properties": {
                    "type": "array",
                    "description": "Array of location id's",
                    "items": {
                      "type": "String",
                      "descrition": "location id"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "error": {
