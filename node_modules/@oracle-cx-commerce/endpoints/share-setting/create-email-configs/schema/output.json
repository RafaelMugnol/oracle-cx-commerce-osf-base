{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-share-setting/createEmailConfigs/output.json",
  "title": "Create Email Configs",
  "description": "Create Email Configs for a given Share Setting.",
  "type": "object",
  "properties": {
    "purchaseListRepository": {
      "type": "object",
      "description": "The purchase list repository",
      "properties": {
        "emailConfigs": {
          "type": "object",
          "description": "The email configs",
          "properties": {
            "emailConfig": {
              "type": "object",
              "description": "The email config",
              "properties": {
                "email": {
                  "description": "Email ID of the recipient.",
                  "type": "string"
                },
                "editEnabled": {
                  "description": "Whether the recipient has edit access for the resource being shared.",
                  "type": "boolean"
                },
                "id": {
                  "description": "ID of the email config.",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "message": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "purchaseListRepository": {
      "emailConfigs": {
        "E1": {"editEnabled": true, "id": "E1", "email": "sam@example.com"}
      }
    }
  }
}
