{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-session/refresh/output.json",
  "type": "object",
  "title": "Refresh",
  "description": "Refresh",
  "properties": {
    "clientRepository": {
      "type": "object",
      "description": "The Client Repository",
      "properties": {
        "context": {
          "type": "object",
          "description": "The Context Object",
          "properties": {
            "session": {
              "type": "object",
              "description": "The Session Object",
              "properties": {
                "token": {
                  "type": "string",
                  "description": "The access token"
                },
                "tokenType": {
                  "type": "string",
                  "description": "The token type"
                },
                "tokenExpires": {
                  "type": "string",
                  "description": "The expires in value"
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "message": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "clientRepository": {
      "context": {
        "session": {
          "token": "access_token123546abcDEF",
          "tokenType": "some_token_type",
          "tokenExpires": "888888"
        }
      }
    },
    "error": {
      "errorCode": "1000500",
      "message": "Invalid input",
      "status": "500"
    }
  }
}