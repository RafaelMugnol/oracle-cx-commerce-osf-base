{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-save-visit-metric/saveVisitsAndVisitorsMetric/input.json",
  "title": "Save Visits And Visitors Metric",
    "description": "Saves visits and visitors information to the repository for analytics purpose.",
    "type": "object",
    "properties": {
      "visitId": {
        "type": "string",
        "description": "Visit ID to be stored"
      },
      "visitorId": {
        "type": "string",
        "description": "Visitor ID to be stored"
      }
    },
    "required": ["visitId","visitorId"],
    "example": {
      "visitId": "visitX",
      "visitorId": "visitorX"
    }
}
