{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-purchaseList/listPurchaseLists/output.json",
  "type": "object",
  "title": "List Purchase List",
  "description": "This API is to get the collection of purchase lists. It can also be used to return the list on a searched string and in a sorted order against various properties of purchase list.",
  "properties": {
    "purchaseListRepository": {
      "type": "object",
      "description": "The Purchase List Repository",
      "properties": {
        "purchaseLists": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "description": "Purchase List Properties for each purchase list",
              "type": "object",
              "properties": {
                "owner": {
                  "description": "The user who created this purchase list.",
                  "type": "object",
                  "properties": {
                    "firstName": {
                      "description": "The first name of the user",
                      "type": "string"
                    },
                    "lastName": {
                      "description": "The last name of the user",
                      "type": "string"
                    },
                    "email": {
                      "description": "The email id of the user",
                      "type": "string"
                    },
                    "repositoryId": {
                      "description": "The Respository Id of the user",
                      "type": "string"
                    }
                  }
                },
                "accountId": {
                  "description": "The ID of the Organization.",
                  "type": "string"
                },
                "lastModifiedDate": {
                  "description": "The last modified date for the purchase list.",
                  "type": "string"
                },
                "lastModifiedBy": {
                  "description": "The user who last modified this purchase list.",
                  "type": "object",
                  "properties": {
                    "firstName": {
                      "description": "The first name of the user",
                      "type": "string"
                    },
                    "lastName": {
                      "description": "The last name of the user",
                      "type": "string"
                    },
                    "email": {
                      "description": "The email id of the user",
                      "type": "string"
                    },
                    "repositoryId": {
                      "description": "The respository ID of the user",
                      "type": "string"
                    }
                  }
                },
                "repositoryId": {
                  "description": "The repository ID of the purchase list.",
                  "type": "string"
                },
                "name": {
                  "description": "The name of the purchase list.",
                  "type": "string"
                },
                "description": {
                  "description": "The description for the purchase list.",
                  "type": "string"
                },
                "siteId": {
                  "description": "The ID of the site.",
                  "type": "string"
                },
                "id": {
                  "description": "The ID of the purchase list.",
                  "type": "string"
                },
                "creationDate": {
                  "description": "The date when purchase list is created.",
                  "type": "string"
                },
                "sharedWithOthers": {
                  "description": "Flag to show if the purchase list is shared with other shoppers or not.",
                  "type": "boolean"
                },
                "items": {
                  "description": "List of purchase list items",
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "productId": {
                        "description": "The product ID",
                        "type": "string"
                      },
                      "quantityDesired": {
                        "description": "The Quantity desired for the item",
                        "type": "integer"
                      },
                      "repositoryId": {
                        "description": "The Repository ID of purchase list item",
                        "type": "boolean"
                      },
                      "id": {
                        "description": "The ID of the purchase list item",
                        "type": "string"
                      },
                      "catRefId": {
                        "description": "The catalog ref ID ",
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "description": "Purchase Lists object"
        }
      }
    }
  },
  "example": {
    "purchaseListRepository": {
