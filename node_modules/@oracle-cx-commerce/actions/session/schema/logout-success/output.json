{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/wapi-session/logoutSuccess/output.json",
  "type": "object",
  "title": "Logout success response",
  "description": "The logout success response. This clears out any order and profile session information.",
  "properties": {
    "orderRepository": {
      "type": "object",
      "description": "The Order Repository",
      "properties": {
        "orders": {
          "type": "object",
          "description": "The Orders Object"
        }
      }
    },
    "profileRepository": {
      "type": "object",
      "description": "The Profile Repository",
      "properties": {
        "profiles": {
          "type": "object",
          "description": "The Profiles Object"
        }
      }
    },
    "clientRepository": {
      "type": "object",
      "description": "The Client Repository",
      "properties": {
        "context": {
          "type": "object",
          "description": "The Context Object",
          "properties": {
            "session": {
              "type": "object",
              "description": "The Session Object",
              "properties": {
                "order": {
                  "type": "object",
                  "description": "The Order Object"
                },
                "profile": {
                  "type": "object",
                  "description": "The Profile Object"
                }
              }
            }
          }
        }
      }
    },
    "error": {
      "type": "object",
      "description": "The error object",
      "properties": {
        "errorCode": {
          "description": "The error code.",
          "type": "string"
        },
        "message": {
          "description": "The error message.",
          "type": "string"
        },
        "status": {
          "description": "The error status.",
          "type": "string"
        }
      }
    }
  },
  "example": {
    "orderRepository": {
      "orders": {}
    },
    "profileRepository": {
      "profiles": {}
    },
    "clientRepository": {
      "context": {
        "session": {
          "order": {},
          "profile": {}
        }
      }
    },
    "error": {
      "errorCode": "1000500",
      "message": "Invalid input",
      "status": "500"
    }
  }
}
