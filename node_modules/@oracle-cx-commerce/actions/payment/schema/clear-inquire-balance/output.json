{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/store-payment/clearInquireBalance/output.json",
  "title": "Clear Inquire Balance",
  "description": "Following model is returned when operation succeeds",
  "type": "object",
  "properties": {
    "profileRepository": {
      "type": "object",
      "description": "The profile repository",
      "properties": {
        "inquireBalanceResponses": {
          "type": "object",
          "description": "The response from the clear inquire balance operation",
          "patternProperties": {
            "^[a-zA-Z1-9]+$": {
              "type": "object",
              "description": "Profile id to which inquired balance belongs to",
              "properties": {
                "giftCards": {
                  "type": "object",
                  "description": "The gift card details. Payment type: physicalGiftCard",
                  "patternProperties": {
                    "^[a-zA-Z1-9]+$": {
                      "type": "object",
                      "description": "The gift card number.Contains the details for this gift card number",
                      "properties": {
                        "type": {
                          "type": "string",
                          "description": "The type of payment. e.g. physicalGiftCard"
                        },
                        "customPaymentProperties": {
                          "type": "object",
                          "description": "The custom payment properties",
                          "properties": {}
                        },
                        "gatewayName": {
                          "type": "string",
                          "description": "The name of the payment gateway. e.g. demoGenericGiftCardGateway"
                        },
                        "paymentMethod": {
                          "type": "string",
                          "description": "The payment method"
                        },
                        "giftCardNumber": {
                          "type": "string",
                          "description": "The gift card number."
                        },
                        "maskedCardNumber": {
                          "type": "string",
                          "description": "The masked card number."
                        },
                        "balance": {
                          "type": "number",
                          "description": "The amount remaining for the git card."
                        }
                      }
                    }
                  }
                },
                "loyaltyPrograms": {
                  "type": "object",
                  "description": "Loyalty details of programs. Payment type: loyaltyPoints",
                  "properties": {
                    "customPaymentProperties": {
                      "type": "object",
                      "description": "The custom payment properties",
                      "properties": {}
                    }
                  },
                  "patternProperties": {
                    "^[a-zA-Z0-9-_]+$": {
                      "type": "object",
                      "description": "The program id. Contains the details for the program",
                      "properties": {
                        "programName": {
                          "type": "string",
                          "description": "The loyalty program name"
                        },
                        "programId": {
                          "type": "string",
                          "description": "The loyalty program id"
                        },
                        "membershipId": {
                          "type": "string",
                          "description": "The membership id"
                        },
                        "additionalProperties": {
                          "type": "object",
                          "description": "Key value pair of additional attributes",
                          "properties": {}
                        },
                        "loyaltyPointDetails": {
                          "type": "array",
                          "description": "The loyalty point details",
                          "items": {
                            "type": "object",
                            "properties": {
                              "membershipType": {
                                "type": "string",
                                "description": "The membership type"
                              },
                              "pointsBalance": {
                                "type": "number",
                                "description": "Available points balance"
                              },
                              "pointsType": {
                                "type": "string",
                                "description": "Points type"
                              },
                              "additionalProperties": {
                                "type": "object",
                                "description": "Key value pair of additional attributes",
                                "properties": {}
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "storeCredits": {
                  "type": "object",
                  "description": "Store credit details. Payment type: storeCredits",
                  "properties": {
                    "customPaymentProperties": {
                      "type": "object",
                      "description": "The custom payment properties",
                      "properties": {}
                    }
                  },
                  "patternProperties": {
                    "^[a-zA-Z0-9-_]+$": {
                      "type": "object",
                      "description": "The store credit number. Contains the details for the store credit",
                      "properties": {
                        "storeCreditNumber": {
                          "type": "string",
                          "description": "Store Credit number"
                        },
                        "balance": {
                          "type": "number",
                          "description": "Amount present in the store credit"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "example": {
    "profileRepository": {
      "inquireBalanceResponses": {
        "se-570031": {
          "giftCards": {},
          "loyaltyPrograms": {
            "prg10001": {
              "loyaltyPointDetails": [
                {
                  "membershipType": "blue",
                  "pointsBalance": 2000,
                  "pointsType": "bluePoints",
                  "additionalProperties": {
                    "propertyName1": "value1",
                    "propertyName2": "value2"
                  }
                },
                {
                  "membershipType": "red",
                  "pointsBalance": 6000,
                  "pointsType": "redPoints",
                  "additionalProperties": {
                    "propertyName4": "value4",
                    "propertyName3": "value3"
                  }
                }
              ],
              "programName": "programForYoungster",
              "membershipId": "member0001",
              "additionalProperties": {
                "propertyName5": "value5",
                "validTill": "2029-11-26T15:57:55.631Z",
                "propertyName6": "value6",
                "validFrom": "2017-07-14T15:57:55.631Z"
              },
              "programId": "prg10001"
            },
            "customPaymentProperties": {
              "prg10001-property-key5": "5000payment-property-value5",
              "cust-prop2": "cust-prop2",
              "prg10001-property-key2": "5000payment-property-value2",
              "cust-prop1": "cust-prop1"
            }
          },
          "storeCredits": {
            "4123654789": {
              "balance": 200,
              "storeCreditNumber": "4123654789"
            },
            "4123654790": {
              "balance": 100,
              "storeCreditNumber": "4123654790"
            },
            "customPaymentProperties": {
              "4123654789-addnl-property-key2": "5000payment-property-value2",
              "4123654790-property-key5": "5000payment-property-value5"
            }
          }
        }
      }
    }
  }
}
