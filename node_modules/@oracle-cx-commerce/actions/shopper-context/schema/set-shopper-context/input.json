{
  "$schema":"http://json-schema.org/draft-06/schema#",
  "id":"https://cloud.oracle.com/commerce-cloud/storefront/schemas/store-shopper-context/setShopperContext/input.json",
  "title":"Set Shopper Context",
  "description":"Set shopper context fetches the PriceListGroup, Catalog and AdditionalPriceListGroups determined by ExternalPriceGroup and Catalog Webhook using current context or event sent by user.",
  "type":"object",
  "properties":{
    "shopperContext":{
      "description":"Shopper Context properties",
      "type":"object"
    }
  },
  "required":[
    "shopperContext"
  ],
  "example":{
    "shopperContext":{
      "defaultAdditionalPriceListGroups":"euroPriceGroup",
      "defaultPriceListGroup":"defaultPriceGroup",
      "defaultCatalog":"VidoeGamesAndCamerasCatalog",
      "message":"message",
      "responseCode":"1"
    }
  }
}
