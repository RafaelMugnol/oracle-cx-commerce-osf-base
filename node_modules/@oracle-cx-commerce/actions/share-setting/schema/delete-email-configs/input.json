{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/store-share-setting/deleteEmailConfigs/input.json",
  "title": "Delete Email Configs",
  "description": "Delete Email Configs for a given Share Setting.",
  "type": "object",
  "properties": {
    "shareSettingId": {
      "type": "string",
      "description": "Share setting to associate the email config with"
    },
    "emailConfigs": {
      "description": "Email configurations to be deleted.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "description": "Id of the email config",
            "type": "string"
          }
        }
      }
    }
  },
  "required": ["shareSettingId", "emailConfigs"],
  "example": {
    "shareSettingId": "1",
    "emailConfigs": [
      {
        "id": "E1"
      }
    ]
  }
}
