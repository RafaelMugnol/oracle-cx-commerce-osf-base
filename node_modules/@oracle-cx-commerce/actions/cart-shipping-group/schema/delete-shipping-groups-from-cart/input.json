{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/store-shipping-group/deleteShippingGroups/input.json",
  "title": "delete Shipping Groups",
  "description": "Delete multiple shipping groups of the current order",
  "type": "object",
  "properties": {
    "shippingGroupsIds": {
      "description": "list of shipping group ids to be deleted",
      "type": "array",
      "properties": {
        "items": {
          "description": "shipping group ID",
          "type": "string"
        }
      }
    }
  },
  "required": [
    "shippingGroupsIds"
  ],
  "example": {
    "shippingGroupsIds": ["sg40421", "sg40422"]
  }
}