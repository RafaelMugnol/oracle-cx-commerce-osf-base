{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "https://cloud.oracle.com/commerce-cloud/storefront/schemas/store-organization-member/removeOrganizationMemberRole/input.json",
  "title": "Remove Organization Member role",
  "description": "Remove Member Roles. This operation is used to remove organization based or global roles from a user",
  "type": "object",
  "properties": {
    "roles": {
      "type": "array",
      "description": "array of roles to be romoved from the member",
      "items": {
        "type": "object",
        "description": "repositoryId details",
        "properties": {
          "id": {
            "description": "organizationId details",
            "type": "string"
          }
        },
        "required": ["id"]
      }
    },
    "memberId": {
      "type": "string",
      "description": "memberId whose role has to be updated" 
    }
  },
  "required": ["memberId"],
  "example": {
    "roles": [
      {
        "id": "org-role-100001"
      },
      {
        "id": "org-role-100002"
      }
    ],
    "memberId": "bb-123412"
  }
}
